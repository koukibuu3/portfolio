{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "import/order": [
      "warn",
      {
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": false
        }
      }
    ]
  }
}
